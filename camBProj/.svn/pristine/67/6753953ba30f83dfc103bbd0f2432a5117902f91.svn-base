<script type="text/javascript">
function objectifyForm(formArray) {//serializeArray data function 
	var returnArray = {}; 
		for (var i = 0; i < formArray.length; i++) { 
			returnArray[formArray[i]['name']] = formArray[i]['value']; 
		} 
	return returnArray;
}

	$(function () {
